{
  "name": "@smart-ai/variable-manager",
  "version": "0.3.0",
  "description": "A JupyterLab extension to manage variables in the kernel, including getting, displaying, deleting, modifying, etc.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/WangChangsongGit/jupyterlab-variable-manager",
  "bugs": {
    "url": "https://github.com/WangChangsongGit/jupyterlab-variable-manager/issues"
  },
  "license": "MIT",
  "author": "WangChangsong",
  "files": [
    "dist/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
